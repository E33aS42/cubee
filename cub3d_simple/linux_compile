clang -Wall -Werror -Wextra ft_cub3d.h get_next_line_utils.c get_next_line.c cub3d_simple.c -lm minilibx-linux-master/libmlx_Linux.a -lXext -lX11

clang -Wall -Werror -Wextra ft_cub3d.h get_next_line_utils.c get_next_line.c ft_cub3d.c -lm minilibx-linux-master/libmlx_Linux.a -lXext -lX11

clang -fsanitize=address -Wall -Werror -Wextra ft_cub3d.h get_next_line_utils.c get_next_line.c ft_cub3d.c -lm minilibx-linux-master/libmlx_Linux.a -lXext -lX11

valgrind -v --leak-check=yes ./a.out cub3d.cub